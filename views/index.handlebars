<div class="container">
  <!--Jumbotron-->
  <div class="row">
  <div class="col-lg-12">
  <div class="jumbotron">
    <h1 id="page-title">Eat-Da-Burger</h1>
  </div>
  </div>

  <div class="row">
  <div class="col-lg-12">
  <img src="/assets/img/cheeseburger.jpg">
  </div>
  </div>

<div class="row">
  <div class="col-lg-12">
<ul>
  {{#each burgers}}
  <li>
    <p>{{burger_name}}</p>
  </li>
  <button class="devour" data-id="{{this.id}}">Devour It!</button>
  {{/each}}
</ul>
</div>
</div>


<div class="row">
<div class="col-lg-12">
<h2>What is a burger you would like to eat?</h2>
<form class="new-burger">
  <input type="text" id ="newburger">
  <button type="submit">Add Burger</button>
</form>
</div>
</div>

</div>

  